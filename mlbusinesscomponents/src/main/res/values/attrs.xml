<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="LoyaltyProgress">
        <attr name="loyaltyNumber" format="integer"/>
        <attr name="sizeLoyaltyNumber" format="dimension"/>
        <attr name="sizeRingStroke" format="dimension"/>
        <attr name="colorLoyaltyProgressText" format="reference"/>
    </declare-styleable>

    <declare-styleable name="MLBusinessDividingLineView">
        <attr name="hasTriangle" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomFilterCellView" tools:ignore="ResourceName">
        <attr name="customFilterCellViewWidth" format="dimension"/>
    </declare-styleable>

    <attr name="pillCornerRadius" format="float" tools:ignore="ResourceName"/>
    <declare-styleable name="RowPillView" tools:ignore="ResourceName">
        <attr name="pillCornerRadius" tools:ignore="ResourceName"/>
    </declare-styleable>
</resources>